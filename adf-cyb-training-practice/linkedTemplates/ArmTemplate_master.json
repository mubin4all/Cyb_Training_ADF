{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-cyb-training-practice"
		},
		"LS_ADLS_AdventureWorks_CybTrainingPrac_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_AdventureWorks_CybTrainingPrac'"
		},
		"LS_ADLS_AdventureWorks_LastModifiedDate_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_AdventureWorks_LastModifiedDate'"
		},
		"LS_ASQLDB_CYB_Training_Practice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQLDB_CYB_Training_Practice'"
		},
		"LS_ADLS_AdventureWorks_CybTrainingPrac_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlscybtrainingpractice.dfs.core.windows.net/"
		},
		"LS_ADLS_AdventureWorks_LastModifiedDate_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlscybtrainingpractice.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-cyb-training-practice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_AdventureWorks_CybTrainingPrac_accountKey": {
						"value": "[parameters('LS_ADLS_AdventureWorks_CybTrainingPrac_accountKey')]"
					},
					"LS_ADLS_AdventureWorks_LastModifiedDate_accountKey": {
						"value": "[parameters('LS_ADLS_AdventureWorks_LastModifiedDate_accountKey')]"
					},
					"LS_ASQLDB_CYB_Training_Practice_connectionString": {
						"value": "[parameters('LS_ASQLDB_CYB_Training_Practice_connectionString')]"
					},
					"LS_ADLS_AdventureWorks_CybTrainingPrac_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_AdventureWorks_CybTrainingPrac_properties_typeProperties_url')]"
					},
					"LS_ADLS_AdventureWorks_LastModifiedDate_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_AdventureWorks_LastModifiedDate_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-cyb-training-practice_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-cyb-training-practice_ArmTemplate_0')]"
			]
		}
	]
}